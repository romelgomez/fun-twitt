datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

model User {
  id      String    @id @default(uuid())
  sort_id String    @unique
  name    String    @default("")
  email   String    @unique
  created DateTime  @default(now())
  updated DateTime  @updatedAt
  deleted DateTime?
}
